node{
   stage('Preparation') {
      echo 'Preparing...'
	  git credentialsId: 'mygit-icp', url: 'https://github.com/ouxwind/newICPdemo.git'
   }

   stage('Unit Test') {
	 echo 'Unit Test...'
     withAnt(installation: 'ant1.10.3') {
          sh '''cd workspace/ICPDemo
		  pwd
          ant junit
          '''
     }
     junit 'workspace/ICPDemo/target/test-reports/TEST-*.xml'
 }
}
